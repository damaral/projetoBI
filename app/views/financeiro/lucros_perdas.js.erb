  	['Fev',#{@receita[2]},#{@custos_variaveis[2]},#{@custos_fixos[2]},#{@impostos[2]},#{@lucros[2]}], 
  	['Mar',#{@receita[3]},#{@custos_variaveis[3]},#{@custos_fixos[3]},#{@impostos[3]},#{@lucros[3]}], 
